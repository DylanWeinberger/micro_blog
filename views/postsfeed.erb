<ul class="postnav">
	<li><a href="/postsfeed">Post Feed</a></li>
	<li><a href="/create">New Post</a></li>
</ul>	

<ul class="feedlist">
<% @posts.each do |post| %>
 <li>
	<div class="feedpostcontainer">
	   <h4><a class="feedtitle" href="/<%= post.id %>"><%= post.title %></a></h4>
	   <p class="feedtimestamp">Created: <%= post.created_at %></p>
	   <% postAuthor = post.user_id %>
	   <p class="feedauthor">Created by: <%= User.find(id: post.user_id).profile.name %></p>
	   <p class="feedcontent"><%= post.content %></p>
	   <% end %>
	</div>
 </li>
</ul>
