<div class="view_profile_info">
	<p>Username: <%= @viewProfileInfo.username %></p>
	<p>Email: <%= @viewProfileInfo.email %></p>
	<p>Birthday <%= @viewProfileInfo.birthday %></p>
	<p>Name: <%= @viewProfileInfo.profile.name + @viewProfileInfo.profile.lname %></p>
	<p>City: <%= @viewProfileInfo.profile.city %></p>
	<p>Age: <%= @viewProfileInfo.profile.age %></p>
</div>	


<ul class="feedlist">
<% @viewProfilePosts.each do |post| %>
 <li>
 	<div class="feedpostcontainer">
	   <h4><a class="feedtitle" href="/<%= post.id %>"><%= post.title %></a></h4>
	   <p class="feedtimestamp">Created: <%= post.created_at %></p>
	   <p class="feedauthor">Created by: <%= post.user_id %></p>
	   <p class="feedcontent"><%= post.content %></p>
	  
	</div>
 </li>
<% end %>
</ul>

